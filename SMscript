	deathFrame.Visible = false
	deathFrame.ZIndex = 1000
	
	deathCorner.CornerRadius = UDim.new(0, 8)
	deathCorner.Parent = deathFrame
	
	deathStroke.Thickness = 3
	deathStroke.Color = Color3.fromRGB(255, 255, 255)
	deathStroke.Parent = deathFrame
	
	deathLabel.Parent = deathFrame
	deathLabel.Size = UDim2.new(1, 0, 1, 0)
	deathLabel.BackgroundTransparency = 1
	deathLabel.Text = "üíÄ –¢–ï–ë–Ø –£–ë–ò–õ: –ù–ï–ò–ó–í–ï–°–¢–ù–û üíÄ"
	deathLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	deathLabel.TextSize = 24
	deathLabel.Font = Enum.Font.GothamBold
	deathLabel.TextScaled = true
end

createWarning()
createDeathMessage()

--------- –§—É–Ω–∫—Ü–∏—è –ø–æ–∫–∞–∑–∞ –ø—Ä–µ–¥—É–ø—Ä–µ–∂–¥–µ–Ω–∏—è
local function showWarning(monsterName, distance)
	warningLabel.Text = "‚ö†Ô∏è " .. monsterName .. " –†–Ø–î–û–ú! [" .. math.floor(distance) .. "m] ‚ö†Ô∏è"
	warningFrame.Visible = true
	
	spawn(function()
		for i = 1, 10 do
			warningFrame.BackgroundTransparency = 0.2 + (i % 2) * 0.3
			wait(0.1)
		end
		warningFrame.Visible = false
	end)
end

--------- –§—É–Ω–∫—Ü–∏—è –ø–æ–∫–∞–∑–∞ —Å–æ–æ–±—â–µ–Ω–∏—è –æ —Å–º–µ—Ä—Ç–∏
local function showDeathMessage(killerName)
	deathLabel.Text = "üíÄ –¢–ï–ë–Ø –£–ë–ò–õ: " .. killerName .. " üíÄ"
	deathFrame.Visible = true
	
	spawn(function()
		wait(3)
		deathFrame.Visible = false
	end)
end

--------- –§—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è ESP
local function createESP(obj, color, customText)
	if processedObjects[obj] then return end
	if obj:FindFirstChild("ESP_Billboard") then return end

	local rootPart = obj
	if obj:IsA("Model") then
		rootPart = obj.PrimaryPart or obj:FindFirstChild("HumanoidRootPart") or obj:FindFirstChild("Torso") or obj:FindFirstChild("Head")
	end

	if not rootPart then return end

	local billboard = Instance.new("BillboardGui")
	billboard.Name = "ESP_Billboard"
	billboard.Parent = rootPart
	billboard.Adornee = rootPart
	billboard.Size = UDim2.new(0, 160, 0, 50)
	billboard.StudsOffset = Vector3.new(0, 2, 0)
	billboard.AlwaysOnTop = true
	billboard.Enabled = true
	billboard.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	billboard.ResetOnSpawn = false

	local frame = Instance.new("Frame")
	frame.Parent = billboard
	frame.Size = UDim2.new(1, 0, 1, 0)
	frame.BackgroundColor3 = theme.Background2
	frame.BackgroundTransparency = 0.1
	frame.BorderSizePixel = 0

	local frameCorner = Instance.new("UICorner")
	frameCorner.CornerRadius = UDim.new(0, 3)
	frameCorner.Parent = frame

	local frameStroke = Instance.new("UIStroke")
	frameStroke.Parent = frame
	frameStroke.Thickness = 1
	frameStroke.Color = theme.Border

	local nameLabel = Instance.new("TextLabel")
	nameLabel.Parent = frame
	nameLabel.Size = UDim2.new(1, -8, 0.5, 0)
	nameLabel.Position = UDim2.new(0, 4, 0, 2)
	nameLabel.BackgroundTransparency = 1
	nameLabel.Text = obj.Name
	nameLabel.TextColor3 = color or theme.Text
	nameLabel.TextSize = 14
	nameLabel.Font = Enum.Font.GothamBold
	nameLabel.TextXAlignment = Enum.TextXAlignment.Left

	local infoLabel = Instance.new("TextLabel")
	infoLabel.Parent = frame
	infoLabel.Size = UDim2.new(1, -8, 0.5, 0)
	infoLabel.Position = UDim2.new(0, 4, 0.5, 0)
	infoLabel.BackgroundTransparency = 1
	infoLabel.Text = (customText or "") .. " | 0m"
	infoLabel.TextColor3 = theme.TextDim
	infoLabel.TextSize = 12
	infoLabel.Font = Enum.Font.Gotham
	infoLabel.TextXAlignment = Enum.TextXAlignment.Left

	local tracer = Instance.new("Frame")
	tracer.Name = "Tracer_" .. obj.Name
	tracer.Parent = screen
	tracer.BackgroundColor3 = color or theme.TextDim
	tracer.BackgroundTransparency = 0.5
	tracer.BorderSizePixel = 0
	tracer.Size = UDim2.new(0, 1, 0, 1)
	tracer.Visible = false
	tracer.ZIndex = 5

	table.insert(espObjects, {
		billboard = billboard, 
		infoLabel = infoLabel, 
		obj = obj, 
		rootPart = rootPart,
		tracer = tracer,
		customText = customText,
		color = color or theme.TextDim,
		isDangerous = false
	})
	
	processedObjects[obj] = true
end

--------- –§—É–Ω–∫—Ü–∏—è –ø—Ä–æ–≤–µ—Ä–∫–∏ –±–ª–∏–∂–∞–π—à–∏—Ö –º–æ–Ω—Å—Ç—Ä–æ–≤ –ø—Ä–∏ –ø–æ–ª—É—á–µ–Ω–∏–∏ —É—Ä–æ–Ω–∞
local function checkNearbyMonsters()
	local character = player.Character
	if not character then return end
	
	local rootPart = character:FindFirstChild("HumanoidRootPart") or character:FindFirstChild("Torso") or character:FindFirstChild("Head")
	if not rootPart then return end
	
	local closestMonster = nil
	local closestDistance = 15 -- –†–∞–¥–∏—É—Å –ø–æ–∏—Å–∫–∞
	local closestName = "–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ"
	
	-- –ü—Ä–æ–≤–µ—Ä—è–µ–º –≤—Å–µ—Ö –ø–æ–¥—Å–≤–µ—á–µ–Ω–Ω—ã—Ö –º–æ–Ω—Å—Ç—Ä–æ–≤
	for _, esp in pairs(espObjects) do
		if esp.isDangerous and esp.rootPart and esp.rootPart.Parent then
			local distance = (rootPart.Position - esp.rootPart.Position).Magnitude
			
			if distance < closestDistance then
				closestDistance = distance
				closestMonster = esp
				closestName = esp.customText or "–ú–æ–Ω—Å—Ç—Ä"
			end
		end
	end
	
	-- –ü—Ä–æ–≤–µ—Ä—è–µ–º Nextbot'–æ–≤ –æ—Ç–¥–µ–ª—å–Ω–æ
	for _, obj in pairs(workspace:GetDescendants()) do
		if obj:IsA("BillboardGui") then
			local parent = obj.Parent
			if parent and parent:IsA("Model") then
				local parentRoot = parent.PrimaryPart or parent:FindFirstChild("HumanoidRootPart") or 
								  parent:FindFirstChild("Torso") or parent:FindFirstChild("Head")
				if parentRoot then
					local distance = (rootPart.Position - parentRoot.Position).Magnitude
					if distance < closestDistance then
						closestDistance = distance
						closestName = "‚ö†Ô∏è NEXTBOT (–†–ê–®) ‚ö†Ô∏è"
					end
				end
			end
		end
	end
	
	if closestMonster or closestName ~= "–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ" then
		lastDamageSource = {
			name = closestName,
			distance = closestDistance,
			time = tick()
		}
		lastDamageTime = tick()
	end
end

--------- –§—É–Ω–∫—Ü–∏—è –Ω–∞—Å—Ç—Ä–æ–π–∫–∏ –¥–µ—Ç–µ–∫—Ç–æ—Ä–∞ —Å–º–µ—Ä—Ç–∏
local function setupDeathDetector()
	local character = player.Character
	if not character then return end
	
	local humanoid = character:FindFirstChild("Humanoid")
	if not humanoid then return end
	
	-- –û—Ç—Å–ª–µ–∂–∏–≤–∞–µ–º –ø–æ–ª—É—á–µ–Ω–∏–µ —É—Ä–æ–Ω–∞
	humanoid:GetPropertyChangedSignal("Health"):Connect(function()
		if humanoid.Health < (humanoid.MaxHealth or 100) then
			checkNearbyMonsters()
		end
	end)
	
	-- –û—Ç—Å–ª–µ–∂–∏–≤–∞–µ–º —Å–º–µ—Ä—Ç—å
	humanoid.Died:Connect(function()
		wait(0.1)
		
		if lastDamageSource and (tick() - lastDamageTime) <= DAMAGE_MEMORY_TIME then
			showDeathMessage(lastDamageSource.name)
			print("üíÄ –¢–ï–ë–Ø –£–ë–ò–õ: " .. lastDamageSource.name .. " (–¥–∏—Å—Ç–∞–Ω—Ü–∏—è: " .. math.floor(lastDamageSource.distance) .. "m)")
		else
			showDeathMessage("–ù–ï–ò–ó–í–ï–°–¢–ù–û (–ø–∞–¥–µ–Ω–∏–µ/–ª–∞–≤–∞)")
			print("üíÄ –¢—ã —É–º–µ—Ä –æ—Ç –Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ–π –ø—Ä–∏—á–∏–Ω—ã")
		end
		
		lastDamageSource = nil
	end)
end

-- –ó–∞–ø—É—Å–∫–∞–µ–º –¥–µ—Ç–µ–∫—Ç–æ—Ä –ø—Ä–∏ –ø–æ—è–≤–ª–µ–Ω–∏–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
player.CharacterAdded:Connect(function(newChar)
	wait(0.5)
	setupDeathDetector()
end)

if player.Character then
	setupDeathDetector()
end

--------- –§—É–Ω–∫—Ü–∏—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è ESP
local function updateESP()
	local character = player.Character
	if not character then return end

	local camera = workspace.CurrentCamera
	if not camera then return end
	
	local rootPart = character:FindFirstChild("HumanoidRootPart") or character:FindFirstChild("Torso") or character:FindFirstChild("Head")
	if not rootPart then return end

	local screenSize = camera.ViewportSize
	local centerBottom = Vector2.new(screenSize.X / 2, screenSize.Y)
	
	local currentTime = tick()
	local closestMonster = nil
	local closestDistance = math.huge
	local closestMonsterName = ""

	for _, esp in pairs(espObjects) do
		if esp.billboard and esp.billboard.Parent and esp.rootPart and esp.rootPart.Parent then
			local distance = (rootPart.Position - esp.rootPart.Position).Magnitude
			esp.infoLabel.Text = (esp.customText or "") .. " | " .. math.floor(distance) .. "m"
			
			if esp.isDangerous and distance < 50 then
				if distance < closestDistance then
					closestDistance = distance
					closestMonster = esp
					closestMonsterName = esp.customText or "–ú–û–ù–°–¢–†"
				end
			end

			if distance > 600 then
				esp.billboard.Enabled = false
				if esp.tracer then esp.tracer.Visible = false end
			else
				esp.billboard.Enabled = true

				if esp.tracer then
					local objPos = camera:WorldToViewportPoint(esp.rootPart.Position)

					if objPos.Z > 0 then
						local screenPos = Vector2.new(objPos.X, objPos.Y)
						local direction = screenPos - centerBottom
						local length = direction.Magnitude
						
						if length > 5 then
							local angle = math.deg(math.atan2(direction.Y, direction.X))
							
							esp.tracer.Size = UDim2.new(0, length, 0, 2)
							esp.tracer.Position = UDim2.new(0, centerBottom.X, 0, centerBottom.Y - 1)
							esp.tracer.Rotation = angle
							esp.tracer.BackgroundColor3 = esp.color
							esp.tracer.BackgroundTransparency = 0.3 + (distance / 600) * 0.5
							esp.tracer.Visible = true
						else
							esp.tracer.Visible = false
						end
					else
						esp.tracer.Visible = false
					end
				end
			end
		end
	end
	
	if closestMonster and currentTime - lastWarningTime > 2 then
		lastWarningTime = currentTime
		showWarning(closestMonsterName, closestDistance)
	end
	
	for i = #espObjects, 1, -1 do
		if not espObjects[i].billboard or not espObjects[i].billboard.Parent then
			if espObjects[i].tracer then espObjects[i].tracer:Destroy() end
			table.remove(espObjects, i)
		end
	end
end

--------- –§—É–Ω–∫—Ü–∏—è –ø–æ–∏—Å–∫–∞ –æ–±—ã—á–Ω—ã—Ö –º–æ–Ω—Å—Ç—Ä–æ–≤
local function findMonsters()
	local found = 0
	for _, obj in pairs(workspace:GetDescendants()) do
		if obj:IsA("Model") and not obj:IsA("Player") and obj ~= player.Character and not processedObjects[obj] then
			
			local displayName, color, dangerous = identifyMonster(obj)
			
			if displayName then
				local rootPart = obj:FindFirstChild("HumanoidRootPart") or obj:FindFirstChild("Torso") or obj:FindFirstChild("Head") or obj.PrimaryPart
				if rootPart then
					createESP(obj, color, displayName)
					
					for _, esp in pairs(espObjects) do
						if esp.obj == obj then
							esp.isDangerous = dangerous
							break
						end
					end
					
					found = found + 1
				end
			end
		end
	end
	return found
end

--------- –§—É–Ω–∫—Ü–∏—è –ø–æ–∏—Å–∫–∞ –ø—Ä–µ–¥–º–µ—Ç–æ–≤
local function findObjectsByName(names, color, defaultText)
	local found = 0
	for _, obj in pairs(workspace:GetDescendants()) do
		if not obj:IsA("Player") and obj.Parent ~= player.Character and not processedObjects[obj] then
			local objName = obj.Name:lower()
			for _, name in pairs(names) do
				local searchName = name:lower()
				if objName == searchName or objName:find(searchName, 1, true) then
					if obj:IsA("Model") or obj:IsA("BasePart") or obj:IsA("MeshPart") or obj:IsA("Tool") then
						createESP(obj, color, defaultText)
						found = found + 1
					end
					break
				end
			end
		end
	end
	return found
end

--------- –§—É–Ω–∫—Ü–∏—è –ø–æ–∏—Å–∫–∞ –∏–Ω—Ç–µ—Ä–∞–∫—Ç–∏–≤–Ω—ã—Ö –æ–±—ä–µ–∫—Ç–æ–≤
local function findInteractObjects()
	local found = 0
	for _, obj in pairs(workspace:GetDescendants()) do
		if not obj:IsA("Player") and obj.Parent ~= player.Character and not processedObjects[obj] then
			
			local hasPrompt = false
			local promptText = ""
			
			if obj:IsA("ProximityPrompt") then
				local parent = obj.Parent
				if parent and parent ~= player.Character and not processedObjects[parent] then
					promptText = obj.ActionText or "Use"
					obj = parent
					hasPrompt = true
				end
			else
				for _, child in pairs(obj:GetDescendants()) do
					if child:IsA("ProximityPrompt") then
						promptText = child.ActionText or "Use"
						hasPrompt = true
						break
					end
				end
			end
			
			if hasPrompt then
				if obj:IsA("Model") then
					local hasParts = false
					for _, child in pairs(obj:GetDescendants()) do
						if child:IsA("BasePart") or child:IsA("MeshPart") then
							hasParts = true
							break
						end
					end
					if hasParts then
						createESP(obj, Color3.fromRGB(180, 180, 255), "[E] " .. promptText)
						found = found + 1
					end
				elseif obj:IsA("BasePart") or obj:IsA("MeshPart") or obj:IsA("Tool") then
					createESP(obj, Color3.fromRGB(180, 180, 255), "[E] " .. promptText)
					found = found + 1
				end
			end
		end
	end
	return found
end

--------- –§—É–Ω–∫—Ü–∏—è –æ—á–∏—Å—Ç–∫–∏ –ø–æ —Ç–∏–ø—É
local function clearESPByType(type)
	for i = #espObjects, 1, -1 do
		local esp = espObjects[i]
		local shouldRemove = false
		
		if type == "Items" and esp.customText and (esp.customText:find("–ü—Ä–µ–¥–º–µ—Ç") or esp.customText:find("ü™ô")) then
			shouldRemove = true
		elseif type == "Coins" and esp.customText and esp.customText:find("ü™ô") then
			shouldRemove = true
		elseif type == "Interact" and esp.customText and esp.customText:find("%[E%]") then
			shouldRemove = true
		elseif type == "Monsters" then
			shouldRemove = true
		end
		
		if shouldRemove then
			if esp.billboard then esp.billboard:Destroy() end
			if esp.tracer then esp.tracer:Destroy() end
			processedObjects[esp.obj] = nil
			table.remove(espObjects, i)
		end
	end
end

--------- –§—É–Ω–∫—Ü–∏—è –ø–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏—è
local function toggleESP(type)
	if type == "Items" then
		if espEnabled.Items then
			espEnabled.Items = false
			ItemsSlider.Text = "OFF"
			ItemsSlider.BackgroundColor3 = theme.Background2
			ItemsSlider.TextColor3 = theme.TextDim
			clearESPByType("Items")
		else
			espEnabled.Items = true
			ItemsSlider.Text = "ON"
			ItemsSlider.BackgroundColor3 = theme.Accent
			ItemsSlider.TextColor3 = Color3.fromRGB(255, 255, 255)
			spawn(function()
				local found = findObjectsByName(itemNames, Color3.fromRGB(255, 200, 100), "üì¶ –ü—Ä–µ–¥–º–µ—Ç")
				if found > 0 then print("‚úÖ –ü—Ä–µ–¥–º–µ—Ç–æ–≤ ESP –Ω–∞–π–¥–µ–Ω–æ:", found) end
			end)
		end

	elseif type == "Coins" then
		if espEnabled.Coins then
			espEnabled.Coins = false
			CoinsSlider.Text = "OFF"
			CoinsSlider.BackgroundColor3 = theme.Background2
			CoinsSlider.TextColor3 = theme.TextDim
			clearESPByType("Coins")
		else
			espEnabled.Coins = true
			CoinsSlider.Text = "ON"
			CoinsSlider.BackgroundColor3 = theme.Accent
			CoinsSlider.TextColor3 = Color3.fromRGB(255, 255, 255)
			spawn(function()
				local found = findObjectsByName(coinNames, Color3.fromRGB(255, 255, 100), "ü™ô –ú–æ–Ω–µ—Ç–∞")
				if found > 0 then print("ü™ô –ú–æ–Ω–µ—Ç ESP –Ω–∞–π–¥–µ–Ω–æ:", found) end
			end)
		end

	elseif type == "Interact" then
		if espEnabled.Interact then
			espEnabled.Interact = false
			InteractSlider.Text = "OFF"
			InteractSlider.BackgroundColor3 = theme.Background2
			InteractSlider.TextColor3 = theme.TextDim
			clearESPByType("Interact")
		else
			espEnabled.Interact = true
			InteractSlider.Text = "ON"
			InteractSlider.BackgroundColor3 = theme.Accent
			InteractSlider.TextColor3 = Color3.fromRGB(255, 255, 255)
			spawn(function()
				local found = findInteractObjects()
				if found > 0 then print("ü§ù –ò–Ω—Ç–µ—Ä–∞–∫—Ç–∏–≤–Ω—ã—Ö –æ–±—ä–µ–∫—Ç–æ–≤ ESP –Ω–∞–π–¥–µ–Ω–æ:", found) end
			end)
		end
		
	elseif type == "Monsters" then
		if espEnabled.Monsters then
			espEnabled.Monsters = false
			MonstersSlider.Text = "OFF"
			MonstersSlider.BackgroundColor3 = theme.Background2
			MonstersSlider.TextColor3 = theme.TextDim
			clearESPByType("Monsters")
		else
			espEnabled.Monsters = true
			MonstersSlider.Text = "ON"
			MonstersSlider.BackgroundColor3 = theme.Accent
			MonstersSlider.TextColor3 = Color3.fromRGB(255, 255, 255)
			spawn(function()
				local found1 = findMonsters()
				local found2 = findNextbots()
				if found1 + found2 > 0 then 
					print("üëπ –ú–æ–Ω—Å—Ç—Ä–æ–≤ ESP –Ω–∞–π–¥–µ–Ω–æ:", found1 + found2, "(Nextbot'–æ–≤:", found2, ")") 
				end
			end)
		end
	end
end

--------- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –Ω–æ–∫–ª–∏–ø–∞
local function toggleNoclip()
	noclipEnabled = not noclipEnabled
	
	if noclipEnabled then
		NoclipSlider.Text = "ON"
		NoclipSlider.BackgroundColor3 = theme.Accent
		NoclipSlider.TextColor3 = Color3.fromRGB(255, 255, 255)
		print("üö™ Noclip –í–ö–õ–Æ–ß–ï–ù")
	else
		NoclipSlider.Text = "OFF"
		NoclipSlider.BackgroundColor3 = theme.Background2
		NoclipSlider.TextColor3 = theme.TextDim
		print("üö™ Noclip –í–´–ö–õ–Æ–ß–ï–ù")
	end
end

--------- –§—É–Ω–∫—Ü–∏—è –Ω–æ–∫–ª–∏–ø–∞
local function noclipLoop()
	local character = player.Character
	if not character then return end
	
	if noclipEnabled then
		for _, part in pairs(character:GetDescendants()) do
			if part:IsA("BasePart") then
				part.CanCollide = false
			end
		end
	else
		for _, part in pairs(character:GetDescendants()) do
			if part:IsA("BasePart") then
				part.CanCollide = true
			end
		end
	end
end

--------- –ú–æ–Ω–∏—Ç–æ—Ä–∏–Ω–≥ –Ω–æ–≤—ã—Ö –æ–±—ä–µ–∫—Ç–æ–≤
local function setupObjectMonitor()
	workspace.DescendantAdded:Connect(function(obj)
		if not processedObjects[obj] and obj.Parent ~= player.Character then
			if espEnabled.Items or espEnabled.Coins or espEnabled.Interact or espEnabled.Monsters then
				spawn(function()
					wait(0.5)
					
					if espEnabled.Items and not processedObjects[obj] then
						local objName = obj.Name:lower()
						for _, name in pairs(itemNames) do
							if objName:find(name:lower(), 1, true) then
								if obj:IsA("Model") or obj:IsA("BasePart") or obj:IsA("MeshPart") or obj:IsA("Tool") then
									createESP(obj, Color3.fromRGB(255, 200, 100), "üì¶ –ü—Ä–µ–¥–º–µ—Ç")
								end
								break
							end
						end
					end
					
					if espEnabled.Coins and not processedObjects[obj] then
						local objName = obj.Name:lower()
						for _, name in pairs(coinNames) do
							if objName:find(name:lower(), 1, true) then
								if obj:IsA("Model") or obj:IsA("BasePart") or obj:IsA("MeshPart") or obj:IsA("Tool") then
									createESP(obj, Color3.fromRGB(255, 255, 100), "ü™ô –ú–æ–Ω–µ—Ç–∞")
								end
								break
							end
						end
					end
					
					if espEnabled.Interact and not processedObjects[obj] then
						local hasPrompt = false
						local promptText = ""
						
						if obj:IsA("ProximityPrompt") then
							local parent = obj.Parent
							if parent and parent ~= player.Character then
								promptText = obj.ActionText or "Use"
								obj = parent
								hasPrompt = true
							end
						else
							for _, child in pairs(obj:GetChildren()) do
								if child:IsA("ProximityPrompt") then
									promptText = child.ActionText or "Use"
									hasPrompt = true
									break
								end
							end
						end
						
						if hasPrompt then
							if obj:IsA("Model") or obj:IsA("BasePart") or obj:IsA("MeshPart") or obj:IsA("Tool") then
								createESP(obj, Color3.fromRGB(180, 180, 255), "[E] " .. promptText)
							end
						end
					end
					
					if espEnabled.Monsters and not processedObjects[obj] then
						if obj:IsA("Model") then
							local displayName, color, dangerous = identifyMonster(obj)
							if displayName then
								local rootPart = obj:FindFirstChild("HumanoidRootPart") or obj:FindFirstChild("Torso") or obj:FindFirstChild("Head") or obj.PrimaryPart
								if rootPart then
									createESP(obj, color, displayName)
									for _, esp in pairs(espObjects) do
										if esp.obj == obj then
											esp.isDangerous = dangerous
											break
										end
									end
								end
							end
						end
						
						if obj:IsA("BillboardGui") then
							local parent = obj.Parent
							if parent and parent:IsA("Model") and not processedObjects[parent] then
								local rootPart = parent.PrimaryPart or parent:FindFirstChild("HumanoidRootPart")
								if rootPart then
									createESP(parent, Color3.fromRGB(255, 0, 0), "‚ö†Ô∏è NEXTBOT (–†–ê–®) ‚ö†Ô∏è")
									for _, esp in pairs(espObjects) do
										if esp.obj == parent then
											esp.isDangerous = true
											break
										end
									end
								end
							end
						end
						
						if obj:IsA("Model") then
							local hasBillboard = false
							for _, child in pairs(obj:GetDescendants()) do
								if child:IsA("BillboardGui") then
									hasBillboard = true
									break
								end
							end
							if hasBillboard and not processedObjects[obj] then
								local rootPart = obj.PrimaryPart or obj:FindFirstChild("HumanoidRootPart")
								if rootPart then
									createESP(obj, Color3.fromRGB(255, 0, 0), "‚ö†Ô∏è NEXTBOT (–†–ê–®) ‚ö†Ô∏è")
									for _, esp in pairs(espObjects) do
										if esp.obj == obj then
											esp.isDangerous = true
											break
										end
									end
								end
							end
						end
					end
				end)
			end
		end
	end)
end

--------- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏—è
local function makeDraggable(draggableObject, dragHandle)
	dragHandle.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragInput = input
			dragStart = input.Position
			startPos = draggableObject.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)

	dragHandle.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement then
			dragInput = input
		end
	end)

	game:GetService("UserInputService").InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			local delta = input.Position - dragStart
			draggableObject.Position = UDim2.new(
				startPos.X.Scale, 
				startPos.X.Offset + delta.X,
				startPos.Y.Scale, 
				startPos.Y.Offset + delta.Y
			)
		end
	end)
end

