--------- –§—É–Ω–∫—Ü–∏—è –æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∏—è –º–æ–Ω—Å—Ç—Ä–∞
local function identifyMonster(obj)
	local objName = obj.Name:lower()
	
	for _, monster in ipairs(monsterDefinitions) do
		for _, keyword in ipairs(monster.keywords) do
			if objName:find(keyword, 1, true) then
				return monster.displayName, monster.color, monster.dangerous
			end
		end
	end
	
	for _, child in pairs(obj:GetDescendants()) do
		local childName = child.Name:lower()
		for _, monster in ipairs(monsterDefinitions) do
			for _, keyword in ipairs(monster.keywords) do
				if childName:find(keyword, 1, true) then
					return monster.displayName, monster.color, monster.dangerous
				end
			end
		end
	end
	
	return nil, nil, false
end

--------- –§—É–Ω–∫—Ü–∏—è –ø–æ–∏—Å–∫–∞ Nextbot'–æ–≤ (–†–∞—à–∞)
local function findNextbots()
	local found = 0
	for _, obj in pairs(workspace:GetDescendants()) do
		if obj:IsA("BillboardGui") and not processedObjects[obj] then
			local parent = obj.Parent
			if parent and parent:IsA("Model") and not processedObjects[parent] then
				local rootPart = parent.PrimaryPart or parent:FindFirstChild("HumanoidRootPart") or 
								 parent:FindFirstChild("Torso") or parent:FindFirstChild("Head")
				if rootPart then
					createESP(parent, Color3.fromRGB(255, 0, 0), "‚ö†Ô∏è NEXTBOT (–†–ê–®) ‚ö†Ô∏è")
					
					for _, esp in pairs(espObjects) do
						if esp.obj == parent then
							esp.isDangerous = true
							break
						end
					end
					
					found = found + 1
				end
			end
		end
		
		if obj:IsA("Model") and not processedObjects[obj] then
			local hasBillboard = false
			for _, child in pairs(obj:GetDescendants()) do
				if child:IsA("BillboardGui") then
					hasBillboard = true
					break
				end
			end
			
			if hasBillboard then
				local rootPart = obj.PrimaryPart or obj:FindFirstChild("HumanoidRootPart") or 
								 obj:FindFirstChild("Torso") or obj:FindFirstChild("Head")
				if rootPart then
					createESP(obj, Color3.fromRGB(255, 0, 0), "‚ö†Ô∏è NEXTBOT (–†–ê–®) ‚ö†Ô∏è")
					
					for _, esp in pairs(espObjects) do
						if esp.obj == obj then
							esp.isDangerous = true
							break
						end
					end
					
					found = found + 1
				end
			end
		end
	end
	return found
end

--------- –§—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è –ø—Ä–µ–¥—É–ø—Ä–µ–∂–¥–µ–Ω–∏—è
local function createWarning()
	warningFrame.Name = "MonsterWarning"
	warningFrame.Parent = screen
	warningFrame.Size = UDim2.new(0, 350, 0, 70)
	warningFrame.Position = UDim2.new(0.5, -175, 0.1, 0)
	warningFrame.BackgroundColor3 = theme.Danger
	warningFrame.BackgroundTransparency = 0.2
	warningFrame.Visible = false
	warningFrame.ZIndex = 1000
	
	warningCorner.CornerRadius = UDim.new(0, 8)
	warningCorner.Parent = warningFrame
	
	warningStroke.Thickness = 3
	warningStroke.Color = Color3.fromRGB(255, 255, 255)
	warningStroke.Parent = warningFrame
	
	warningLabel.Parent = warningFrame
	warningLabel.Size = UDim2.new(1, 0, 1, 0)
	warningLabel.BackgroundTransparency = 1
	warningLabel.Text = "‚ö†Ô∏è NEXTBOT –†–Ø–î–û–ú! ‚ö†Ô∏è"
	warningLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	warningLabel.TextSize = 20
	warningLabel.Font = Enum.Font.GothamBold
	warningLabel.TextScaled = true
end

--------- –§—É–Ω–∫—Ü–∏—è —Å–æ–∑–¥–∞–Ω–∏—è —Å–æ–æ–±—â–µ–Ω–∏—è –æ —Å–º–µ—Ä—Ç–∏
